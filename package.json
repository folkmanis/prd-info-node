{
  "name": "prd-info-node",
  "version": "2.0.0",
  "description": "prd info REST module",
  "main": "dist/start.js",
  "scripts": {
    "build:watch": "nest build --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/folkmanis/prd-info-node.git"
  },
  "keywords": [
    "xmf",
    "nodejs",
    "node",
    "archive"
  ],
  "author": "Edmunds",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/folkmanis/prd-info-node/issues"
  },
  "homepage": "https://github.com/folkmanis/prd-info-node#readme",
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/platform-ws": "^9.0.8",
    "@nestjs/websockets": "^9.0.8",
    "busboy": "^1.4.0",
    "chokidar": "^3.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-mongo": "^5.0.0",
    "date-fns": "^2.28.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "flat": "^5.0.2",
    "google-auth-library": "^8.0.2",
    "googleapis": "^118.0.0",
    "googleapis-common": "^6.0.0",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "mongodb": "^5.4.0",
    "nestjs": "^0.0.1",
    "objtree": "^0.1.24",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pdfmake": "^0.1.72",
    "pdfmake-wrapper": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "sanitize-filename": "^1.6.3",
    "ws": "^8.4.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/testing": "^9.0.8",
    "@types/busboy": "^1.3.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/flat": "^5.0.2",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^18.16.6",
    "@types/passport-local": "^1.0.34",
    "@types/pdfmake": "^0.1.21",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "src",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(\\.|/)(spec|test)\\.(ts|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}
