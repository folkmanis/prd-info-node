{
  "name": "prd-info-node",
  "version": "2.0.0",
  "description": "prd info REST module",
  "main": "dist/start.js",
  "scripts": {
    "build:watch": "nest build --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/folkmanis/prd-info-node.git"
  },
  "keywords": [
    "xmf",
    "nodejs",
    "node",
    "archive"
  ],
  "author": "Edmunds",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/folkmanis/prd-info-node/issues"
  },
  "homepage": "https://github.com/folkmanis/prd-info-node#readme",
  "dependencies": {
    "@nestjs/common": "^8.2.5",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.5",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.5",
    "@nestjs/platform-ws": "^8.2.5",
    "@nestjs/websockets": "^8.2.5",
    "@overnightjs/core": "^1.7.6",
    "@types/busboy": "^0.2.4",
    "@types/dotenv": "^8.2.0",
    "@types/express-session": "^1.17.4",
    "@types/lodash": "^4.14.178",
    "@types/tmp": "^0.2.3",
    "@types/ws": "^8.2.2",
    "body-parser": "^1.19.1",
    "busboy": "^0.3.1",
    "chokidar": "^3.5.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "connect-mongo": "^4.6.0",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "joi": "^17.5.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.3.0",
    "objtree": "^0.1.24",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pdfmake": "^0.2.4",
    "pdfmake-wrapper": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "sanitize-filename": "^1.6.3",
    "tmp-promise": "^3.0.3",
    "ws": "^8.4.0",
    "xlsx": "^0.17.5",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.8",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.18.5",
    "@types/passport-local": "^1.0.34",
    "@types/pdfmake": "^0.1.20",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "supertest": "^6.2.0",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "src",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(\\.|/)(spec|test)\\.(ts|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage"
  }
}
